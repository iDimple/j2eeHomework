<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
	<package name="struts2" namespace="/homework" extends="struts-default">
		<action name="login" class="loginAction">
			<result name="success">/view/CourseList.jsp</result>
			<result name="error">/view/NoSuchUser.jsp</result>
			<result name="login">/view/Login.jsp</result>
		</action>
		
		<action name="ShowResults" class="showResultsAction">
			<result name="success">/view/ShowResult.jsp</result>
			<result name="error">/view/Login.jsp</result>
		</action>
		
	</package>
</struts>

